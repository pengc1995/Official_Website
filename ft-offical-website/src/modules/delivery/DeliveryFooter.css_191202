/* Delivery Footer */

.fd-footer-bg {
    background-color:#062236;
}
.fd-footer-content {
    margin: 0;
    padding: 0;
    padding-top: 60px;
    padding-bottom: 60px;
    max-width: 1140px;
    margin: 0 auto;
    /* position: relative; */
}
  

/* Process group 1 */
/* --------------- */

.fd-footer-group-1 {
    margin: 0;
    padding: 0;

    display: -webkit-flex;
    display: flex;  

    -webkit-flex-direction: row;
    flex-direction: row;

    -webkit-flex-wrap: wrap;  
    flex-wrap: wrap;  

    -webkit-justify-content: space-between;
    justify-content: space-between;
    
    -webkit-align-items: flex-start;
    align-items: flex-start;  
}

.ft-w1140-button-fs16 {
    color: white;
}

.fd-footer-1to4 {
    /* flex-grow: 1; */
    /* width: 213px; */
    margin-bottom: 20px;
}
